    type: 'line',
    maintainAspectRatio: true,
    responsive: true,
    data: {
        labels: dia,
        datasets: [{
            data: meta,
            label: "Status",
            borderColor: "#CD5C5C",
            fill: false,
            pointStyle: 'circle',
            radius: '0',
            borderDash: [5],
            borderWidth: 2
